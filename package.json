{
  "name": "hdfcBackEnd",
  "version": "1.0.0",
  "description": "Hdfc developer portal api",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:prod": "node build/index.js",
    "compile": "cp .env.dev .env &&  babel server --out-dir dist --delete-dir-on-start --source-maps inline --copy-files",
    "compile:prod": "cp .env.prod .env &&  babel server --out-dir build --delete-dir-on-start --source-maps inline --copy-files",
    "dev": "cp .env.local .env && nodemon server --exec babel-node --config .nodemonrc.json | pino-pretty",
    "dev:debug": "nodemon server --exec babel-node --config .nodemonrc.json --inspect | pino-pretty",
    "test": "mocha --timeout 10000 --require @babel/register --exit ",
    "test:debug": "mocha --require @babel/register --inspect-brk --exit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "stop": "pkill node",
    "seed:states": "node seeders/stateSeeder.js",
    "seed:cities": "node seeders/citySeeder.js",
    "seed:districts": "node seeders/districtSeeder.js",
    "seed:talukas": "node seeders/talukaSeeder.js",
    "seed:areas": "node seeders/areaSeeder.js",
    "seed:pincodes": "node seeders/pincodeSeeder.js"
  },
  "dependencies": {
    "axios": "^1.2.4",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "date-and-time": "^2.4.1",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "express-openapi-validator": "^4.13.8",
    "express-validator": "^6.14.2",
    "form-data": "^4.0.0",
    "fs": "^0.0.1-security",
    "jsforce": "^1.11.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "otp-generator": "^4.0.0",
    "password-validator": "^5.3.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "pino": "^8.0.0",
    "pino-http": "^8.3.3",
    "pino-pg": "^2.2.1",
    "query-string": "^8.1.0",
    "read-excel-file": "^5.6.1",
    "sequelize": "^6.26.0",
    "uuid-token-generator": "^1.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.2",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/preset-env": "^7.14.4",
    "@babel/register": "^7.13.16",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.7",
    "pino-pretty": "^7.5.1",
    "prettier": "^2.3.0",
    "supertest": "^6.1.3"
  },
  "author": "centelon digital team <khaleel.ik@centelon.com/antony.thomas@centelon.com>"
}
